  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "next dev",
    "dev:backend": "node -r dotenv/config src/backend/server.js dotenv_config_path=src/backend/.env",
    "build": "next build",
    "start": "concurrently \"node -r dotenv/config src/backend/server.js dotenv_config_path=src/backend/.env\" \"next start\"",
    "lint": "next lint"
  },
  "dependencies": {
    "@faker-js/faker": "^9.3.0",
    "@headlessui/react": "^2.2.0",
    "@heroicons/react": "^2.2.0",
    "@tiptap/react": "^2.11.0",
    "@tiptap/starter-kit": "^2.11.0",
    "axios": "^1.7.9",
    "concurrently": "^9.1.1",
    "dotenv": "^16.4.7",
    "faker": "^6.6.6",
    "firebase": "^11.1.0",
    "next": "15.1.3",
    "next-cloudinary": "^6.16.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "autoprefixer": "^10.4.20",
    "eslint": "^9",
